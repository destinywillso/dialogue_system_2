  select_negative_feedback: ({ is }) => {


    const lastInput = is.input[is.input.length - 1];
    if (lastInput === "*no_input*" || lastInput === "ASR_NOINPUT") {
      const lastQ = is.shared.qud[0];
      const message = lastQ
        ? `I didn't hear anything from you. ${lastQ.predicate}`
        : "I didn't hear anything from you.";
      const move: Move = { type: "negative_feedback", content: message };
      return () => ({
        ...is,
        next_moves: [...is.next_moves, move],
      });
    }
  },